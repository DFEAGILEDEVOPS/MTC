{
  "name": "mtc-admin-app",
  "version": "1.2.0",
  "private": false,
  "license": "GPL-3.0",
  "scripts": {
    "build": "gulp build",
    "build:watch": "yarn tsc --watch",
    "cleanbuild": "yarn clean && yarn build",
    "dev": "yarn start:dev",
    "dev-build": "gulp dev-build",
    "shell-build": "./build.sh",
    "clean": "rm -rf dist; rm -f .env",
    "lint": "standard && yarn lint:ts",
    "lint:ts": "yarn eslint .",
    "lintAndBuild": "yarn lint && yarn build",
    "migrate-sql": "node ../db/engine/migrate.js",
    "migrate-and-seed-sql": "yarn migrate-sql && yarn seed-sql",
    "purgestorage": "bin/init-dev-storage.js",
    "seed-sql": "node ../db/engine/seed.js",
    "sasswatch": "gulp watch",
    "start:js": "node --max-http-header-size 15000 ./server.js",
    "start": "node --max-http-header-size 15000 ./dist/server.js",
    "start:dev": "yarn dev-build && concurrently -r 'nodemon --inspect --delay 1500ms --watch ./dist --max-http-header-size 15000 ./dist/server.js' 'gulp watch'",
    "test": "yarn lint && yarn jest",
    "test:watch": "yarn jest --watch --verbose",
    "test:coverage": "yarn lint && yarn jest --coverage",
    "test:integration": "yarn jest --config ./tests-integration/jest.integration.config.js",
    "test:frontend": "yarn run gulp bundleAppJsForCodeCoverage && karma start karma.conf.js",
    "test:all": "yarn clean && yarn test && yarn test:integration && yarn test:frontend && yarn build",
    "watch": "yarn test:watch",
    "watch:integration": "yarn jest --watch --coverage=no --config ./tests-integration/jest.integration.config.js"
  },
  "mtc": {
    "assets-version": "6a569a48061e8e7844eee749fb4527b3"
  },
  "engines": {
    "node": ">= 18"
  },
  "resolutions": {
    "**/**/marked": "4.3.0"
  },
  "dependencies": {
    "@azure/data-tables": "^13.2.1",
    "@azure/service-bus": "^7.9.1",
    "@azure/storage-blob": "12.18.0",
    "@azure/storage-queue": "12.17.0",
    "adm-zip": "^0.5.10",
    "applicationinsights": "^3.3.0",
    "axios": "^1.7.8",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "connect-flash": "^0.1.1",
    "connect-redis": "^8.0.0",
    "dompurify": "^3.1.7",
    "dotenv": "^16.0.3",
    "edit-json-file": "^1.4.0",
    "ejs": "^3.1.8",
    "express": "^4.21.2",
    "express-breadcrumbs": "https://github.com/DFEAGILEDEVOPS/express-breadcrumbs.git",
    "express-busboy": "^9.0.0",
    "express-partials": "^0.3.0",
    "express-session": "^1.18.1",
    "express-winston": "^4.1.0",
    "expressjs-csurf": "https://github.com/DFEAGILEDEVOPS/expressjs-csurf#v2.0.1",
    "fast-csv": "^4.1.1",
    "feature-toggles": "^1.4.0",
    "fflate": "^0.8.2",
    "font-awesome": "^4.7.0",
    "fs-extra": "^11.1.0",
    "govuk-frontend": "^3.2.0",
    "helmet": "^4.3.1",
    "ioredis": "5.4.1",
    "jsdom": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "login.dfe.async-retry": "git+https://github.com/DFE-Digital/login.dfe.async-retry.git#v1.0",
    "lz-string": "^1.4.4",
    "marked": "4.3.0",
    "md5": "^2.2.1",
    "merge-stream": "^2.0.0",
    "moment-timezone": "^0.5.46",
    "morgan": "~1.10.0",
    "mssql": "10.0.4",
    "nocache": "^3.0.4",
    "node-dir": "^0.1.17",
    "openid-client": "^4.2.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "qrcode": "^1.5.1",
    "ramda": "^0.30.1",
    "ramda-adjunct": "^5.1.0",
    "random-number-csprng": "^1.0.2",
    "sass": "^1.80.3",
    "simplemde": "^1.11.2",
    "to-bool": "^0.0.1",
    "uuid": "^10.0.0",
    "uuid-validate": "^0.0.3",
    "validator": "^13.7.0",
    "winston": "^3.15.0",
    "xmlbuilder2": "^3.0.2",
    "xregexp": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.16.0",
    "@faker-js/faker": "^7.6.0",
    "@types/adm-zip": "^0.4.34",
    "@types/dompurify": "^2.3.1",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^27.0.1",
    "@types/jsdom": "^16.2.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "18.11.9",
    "@types/ramda": "^0.30.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^27.4.2",
    "chalk": "^4.1.0",
    "command-line-args": "^5.0.2",
    "concurrently": "^6.2.1",
    "csv-string": "^4.0.1",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sass-variables": "^1.1.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.0",
    "iconv-lite": "^0.6.3",
    "jest": "^27.0.6",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-remap-istanbul": "~0.6.0",
    "node-mocks-http": "^1.6.6",
    "nodemon": "^2.0.12",
    "standard": "^17.0.0",
    "supertest": "^6.0.1",
    "ts-node": "^10.4.0",
    "typescript": "4.5.5"
  },
  "standard": {
    "ignore": [
      "assets/vendor-js/*",
      "**/*.ts",
      ".eslintrc.js"
    ]
  },
  "jest": {
    "bail": true,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/data-access/**",
      "!deploy/**",
      "!data/**",
      "!assets/**",
      "!public/**",
      "!spec/front-end/**",
      "!**/*.spec.js",
      "!**/tests-integration/**",
      "!**/back-end/mocks/**",
      "!**/coverage/**",
      "!**/bin/**",
      "!<rootDir>/dist/**",
      "!<rootDir>/routes/**",
      "!**/*.data.service.[jt]s"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "testMatch": [
      "**/spec/back-end/**/(*.)+(spec|test).[jt]s?(x)",
      "**/services/*/*.spec.[jt]s",
      "**/services/**/*.spec.[jt]s"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/spec/back-end/mocks/",
      "<rootDir>/spec/front-end",
      "<rootDir>/spec/back-end/coverage",
      "<rootDir>/tests/integration",
      "<rootDir>/services/data-access",
      "<rootDir>/dist"
    ],
    "verbose": false,
    "testRunner": "jest-circus",
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  }
}