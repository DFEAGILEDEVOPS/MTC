{
  "name": "check-development-app",
  "version": "1.2.0",
  "private": false,
  "license": "GPL-3.0",
  "scripts": {
    "lint": "standard",
    "migrate-sql": "node ./data/sql/migrate-sql.js",
    "seed-sql": "node ./data/sql/seed-sql.js",
    "migrate-and-seed-sql": "yarn migrate-sql && yarn seed-sql",
    "postinstall": "gulp build",
    "sasswatch": "gulp watch",
    "start": "node --max-http-header-size 15000 ./server.js",
    "startd": "yarn migrate-sql && yarn start",
    "test": "yarn lint && yarn jest",
    "test:watch": "yarn jest --watch --coverage=no",
    "test:nocoverage": "yarn lint && yarn jest --coverage=no",
    "test:integration": "yarn jest --config ./tests-integration/jest.integration.config.js",
    "test:frontend": "karma start karma.conf.js",
    "test:all": "yarn test && yarn test:integration && yarn test:frontend",
    "purgestorage": "bin/init-dev-storage.js",
    "debug": "node --inspect=0.0.0.0:9229 ./server.js",
    "parse-school-changes-csv": "node bin/parse-school-changes-csv.js",
    "watch": "yarn test:watch"
  },
  "mtc": {
    "assets-version": "dfb8d1dc1f8725f8b0813fdebb08f8fd"
