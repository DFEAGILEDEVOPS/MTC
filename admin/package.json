{
  "name": "check-development-app",
  "version": "1.2.0",
  "private": false,
  "license": "GPL-3.0",
  "scripts": {
    "build": "gulp build",
    "dev-build": "gulp dev-build",
    "shell-build": "./build.sh",
    "clean": "rm -rf dist; rm -f .env",
    "lint": "standard",
    "lintAndBuild": "yarn lint && yarn build",
    "migrate-sql": "node ../db/engine/migrate.js",
    "migrate-and-seed-sql": "yarn migrate-sql && yarn seed-sql",
    "postinstall": "gulp build",
    "purgestorage": "bin/init-dev-storage.js",
    "seed-sql": "node ../db/engine/seed.js",
    "sasswatch": "gulp watch",
    "start:js": "node --max-http-header-size 15000 ./server.js",
    "start": "yarn build && node --max-http-header-size 15000 ./dist/server.js",
    "start:dev": "yarn dev-build && concurrently -r 'nodemon --inspect --delay 1500ms --watch ./dist --max-http-header-size 15000 ./dist/server.js' 'gulp watch'",
    "test": "yarn lint && yarn jest",
    "test:watch": "yarn jest --watch",
    "test:coverage": "yarn lint && yarn jest --coverage",
    "test:integration": "yarn jest --config ./tests-integration/jest.integration.config.js",
    "test:frontend": "yarn run gulp bundleAppJsForCodeCoverage && karma start karma.conf.js",
    "test:all": "yarn clean && yarn test && yarn test:integration && yarn test:frontend && yarn build",
    "watch": "yarn test:watch",
    "watch:integration": "yarn jest --watch --coverage=no --config ./tests-integration/jest.integration.config.js"
  },
  "mtc": {
    "assets-version": "d751ca26ae5c9e59b075d9cdb5f7c021"
  },
  "engines": {
    "node": ">= 14"
  },
  "resolutions": {
    "**/**/lodash": "^4.17.21",
    "**/**/ajv": "^6.12.6"
  },
