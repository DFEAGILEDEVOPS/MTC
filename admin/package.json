    "gulp-standard": "^10.1.1",
    "gulp-uglify": "^3.0.0",
    "iconv-lite": "^0.4.19",
    "jsonwebtoken": "^7.4.3",
    "migrate-mongo": "^2.2.1",
    "moment": "^2.18.1",
    "mongoose": "4.9.6",
    "mongoose-auto-increment": "^5.0.1",
    "morgan": "~1.8.1",
    "newrelic": "^1.40.0",
    "node-gyp": "^3.6.1",
    "number-to-words": "^1.2.3",
    "passport": "^0.3.2",
    "passport-custom": "^1.0.5",
    "passport-local": "^1.0.0",
    "piping": "^1.0.0-rc.4",
    "serve-favicon": "^2.4.4",
    "uuid": "^3.1.0",
    "xregexp": "^3.2.0"
