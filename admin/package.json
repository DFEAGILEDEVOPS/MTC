    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  },
  "standard": {
    "ignore": [
      "assets/javascripts/jquery-1.12.4.js"
    ]
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/data-access/**",
      "!deploy/**",
      "!data/**",
      "!assets/**",
      "!public/**",
      "!spec/front-end/**",
      "!**/*.spec.js",
      "!**/tests-integration/**",
      "!**/back-end/mocks/**",
      "!**/coverage/**",
      "!**/bin/**"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "testMatch": [
      "**/spec/back-end/**/(*.)+(spec|test).[jt]s?(x)",
      "**/services/*/*.spec.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/spec/back-end/mocks/",
      "<rootDir>/spec/front-end",
      "<rootDir>/spec/back-end/coverage",
      "<rootDir>/tests/integration",
      "<rootDir>/services/data-access"
    ],
    "verbose": false
  }
}