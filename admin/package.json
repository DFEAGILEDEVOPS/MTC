{
  "name": "check-development-app",
  "version": "1.2.0",
  "private": false,
  "license": "GPL-3.0",
  "scripts": {
    "build": "gulp build",
    "dev-build": "gulp dev-build",
    "shell-build": "./build.sh",
    "clean": "rm -rf dist; rm -f .env",
    "lint": "standard",
    "lintAndBuild": "yarn lint && yarn build",
    "migrate-sql": "node ../db/engine/migrate.js",
    "migrate-and-seed-sql": "yarn migrate-sql && yarn seed-sql",
    "postinstall": "gulp build",
    "purgestorage": "bin/init-dev-storage.js",
    "seed-sql": "node ../db/engine/seed.js",
    "sasswatch": "gulp watch",
    "start:js": "node --max-http-header-size 15000 ./server.js",
    "start": "yarn build && node --max-http-header-size 15000 ./dist/server.js",
    "start:dev": "yarn dev-build && concurrently -r 'nodemon --inspect --delay 1500ms --watch ./dist --max-http-header-size 15000 ./dist/server.js' 'gulp watch'",
    "test": "yarn lint && yarn jest",
    "test:watch": "yarn jest --watch --coverage=no",
    "test:nocoverage": "yarn lint && yarn jest --coverage=no",
    "test:integration": "yarn jest --config ./tests-integration/jest.integration.config.js",
    "test:frontend": "karma start karma.conf.js",
    "test:all": "yarn clean && yarn test && yarn test:integration && yarn test:frontend && yarn build",
    "watch": "yarn test:watch",
    "watch:integration": "yarn jest --watch --coverage=no --config ./tests-integration/jest.integration.config.js"
  },
  "mtc": {
    "assets-version": "0fc75a9344f8f2567e1ad3fd7aff0372"
  },
  "engines": {
    "node": ">= 12"
  },
  "resolutions": {
    "**/**/lodash": "^4.17.21",
    "**/**/ajv": "^6.12.6"
  },
  "dependencies": {
    "@azure/service-bus": "^7.0.5",
    "applicationinsights": "^2.1.5",
    "axios": "^0.21.1",
    "azure-storage": "^2.10.4",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "connect-flash": "^0.1.1",
    "connect-redis": "^6.0.0",
    "csurf": "^1.9.0",
    "dotenv": "^10.0.0",
    "edit-json-file": "^1.4.0",
    "ejs": "^3.1.6",
    "express": "^4.16.2",
    "express-breadcrumbs": "https://github.com/DFEAGILEDEVOPS/express-breadcrumbs.git",
    "express-busboy": "^8.0.0",
    "express-partials": "^0.3.0",
    "express-session": "^1.15.6",
    "express-winston": "^4.1.0",
    "fast-csv": "^4.1.1",
    "feature-toggles": "^1.4.0",
    "fs-extra": "^10.0.0",
    "govuk-frontend": "^3.2.0",
    "helmet": "^4.3.1",
    "ioredis": "4.27.8",
    "jsonwebtoken": "^8.1.1",
    "login.dfe.async-retry": "git+https://github.com/DFE-Digital/login.dfe.async-retry.git#v1.0",
    "lz-string": "^1.4.4",
    "md5": "^2.2.1",
    "merge-stream": "^2.0.0",
    "moment-timezone": "^0.5.23",
    "morgan": "~1.10.0",
    "mssql": "7.2.0",
    "nocache": "^3.0.1",
    "node-dir": "^0.1.17",
    "openid-client": "^4.2.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "qrcode": "^1.0.0",
    "ramda": "^0.27.0",
    "ramda-adjunct": "^2.21.0",
    "random-number-csprng": "^1.0.2",
    "sass": "^1.38.0",
    "to-bool": "^0.0.1",
    "uuid": "^8.3.2",
    "uuid-validate": "^0.0.3",
    "validator": "^13.5.2",
    "winston": "^3.1.0",
    "winston-azure-application-insights": "^3.0.0",
    "xmlbuilder2": "^3.0.2",
    "xregexp": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^12.7.12",
    "@types/ramda": "^0.27.6",
    "chalk": "^4.1.0",
    "command-line-args": "^5.0.2",
    "concurrently": "^6.2.1",
    "csv-string": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sass-variables": "^1.1.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.0",
    "jest": "^27.0.6",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "node-mocks-http": "^1.6.6",
    "proxyquire": "^2.1.0",
    "sinon": "^11.1.2",
    "standard": "^16.0.3",
    "supertest": "^6.0.1",
    "typescript": "^4.1.3"
  },
  "standard": {
    "ignore": [
      "assets/javascripts/jquery-3.5.1.js"
    ]
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/data-access/**",
      "!deploy/**",
      "!data/**",
      "!assets/**",
      "!public/**",
      "!spec/front-end/**",
      "!**/*.spec.js",
      "!**/tests-integration/**",
      "!**/back-end/mocks/**",
      "!**/coverage/**",
      "!**/bin/**",
      "!<rootDir>/dist/**",
      "!<rootDir>/routes/**"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "testMatch": [
      "**/spec/back-end/**/(*.)+(spec|test).[jt]s?(x)",
      "**/services/*/*.spec.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/spec/back-end/mocks/",
      "<rootDir>/spec/front-end",
      "<rootDir>/spec/back-end/coverage",
      "<rootDir>/tests/integration",
      "<rootDir>/services/data-access",
      "<rootDir>/dist"
    ],
    "verbose": false,
    "testRunner": "jest-jasmine2"
  }
}