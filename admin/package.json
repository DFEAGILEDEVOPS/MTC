    "testDebug": "node $NODE_DEBUG_OPTION ./node_modules/.bin/jasmine",
    "purgestorage": "bin/init-dev-storage.js",
    "debug": "node --inspect=0.0.0.0:9229 ./server.js",
    "parse-school-changes-csv": "node bin/parse-school-changes-csv.js"
  },
