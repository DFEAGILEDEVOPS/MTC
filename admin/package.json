  "devDependencies": {
    "jasmine-node": "^1.14.5",
    "jsdoc": "^3.5.5",
    "levenshtein": "^1.0.5",
    "node-mocks-http": "^1.6.4",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "proxyquire": "^1.8.0",
    "sinon": "^3.2.1",
    "sinon-mongoose": "^2.0.2",
    "standard": "^10.0.2"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*Spec.js",
      "newrelic.js",
      "gulpfile.js",
      "bin/**/*",
      "public/govuk_template/**/*",
      "public/javascripts/app.js",
      "assets/javascripts/details.polyfill.js",
      "assets/javascripts/jquery-1.12.4.js",
      "assets/javascripts/modernizr-custom.js",
      "app.js",
      "server.js",
      "data/migrations/",
      "spec/mocks/*"
    ],
    "all": true,
    "cache": true
  },
  "standard": {
    "ignore": [
      "public/govuk_template/**/*",
      "assets/javascripts/details.polyfill.js",
      "assets/javascripts/jquery-1.12.4.js"
    ]
  }
