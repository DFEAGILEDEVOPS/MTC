{
  "name": "check-development-app",
  "version": "0.3.0",
  "private": false,
  "license": "GPL-3.0",
  "scripts": {
    "prestart": "cd data/migrations && ../../node_modules/.bin/migrate-mongo up",
    "start": "node ./server.js",
    "postinstall": "gulp build",
    "test": "nyc -r html jasmine-node spec --verbose --color --captureExceptions --forceexit",
    "coverage": "nyc -r text -r html jasmine-node spec --verbose --color --captureExceptions --forceexit",
    "startn": "nodemon ./server.js",
    "sasswatch": "gulp watch"
  },
  "engines": {
    "node": "8.4.0"
  },
  "dependencies": {
