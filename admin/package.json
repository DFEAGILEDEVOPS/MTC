  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*Spec.js",
      "app.js",
      "assets/javascripts/details.polyfill.js",
      "assets/javascripts/jquery-1.12.4.js",
      "assets/javascripts/modernizr-custom.js",
      "bin/**/*",
      "coverage/**/*",
      "data/**/*",
      "gulpfile.js",
      "public/javascripts/app.js",
      "server.js",
      "spec/mocks/*",
      "tests-integration/*",
      "services/data-access",
      "routes"
