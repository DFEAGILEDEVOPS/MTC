<%- partial('partials/_breadcrumbs_and_sign_out', { breadcrumbs }) %>

<div class="grid-row">
    <div class="column-two-thirds">
        <h1 class="heading-xlarge">Multiplication tables check for test development</h1>

        <%- partial('partials/_session_expiry') %>

        <ol class="list">
            <% const uploadAndViewFormsHrefValue = isNewCheckFormFeatureToggleEnabled ? '/check-form/view-forms' : '/test-developer/upload-and-view-forms' %>
            <li>
                <h2 class="heading-small"><a  href="<%= uploadAndViewFormsHrefValue %>">Upload and view forms</a></h2>
                <p>Add, view, remove and edit check forms</p>
            </li>

            <li>
              <% const assignFormsHrefValue = isNewCheckFormFeatureToggleEnabled ? '/check-form/assign-forms-to-check-windows' : '/test-developer/assign-form-to-window' %>
              <h2 class="heading-small"><a href="<%= assignFormsHrefValue %>">Assign forms to check windows</a></h2>
                <p>Select uploaded forms and assign them to check windows</p>
            </li>

            <li>
                <h2 class="heading-small"><a href="/test-developer/download-pupil-check-data">Download pupil check data</a></h2>
                <p>Generate and download detailed raw pupil check data</p>
            </li>
        </ol>
    </div>

    <div class="column-one-third sidebar" aria-label="sidebar">
        <aside role="complementary" class="support-column">
            <h3 class="heading-medium">Related</h3>
            <nav role="navigation">
                <ul class="list">
                    <li>
                        <a href="<%- guidancePdf %>" target="_blank" rel="noopener noreferrer">Guidance (PDF)</a>
                    </li>
                </ul>
            </nav>
        </aside>
    </div>
</div>
