<head>
  <meta charset="utf-8" />
  <title><%- partial('partials/_page_title') %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
  <meta name="theme-color" content="#0b0c0c" />

  <meta http-equiv="X-UA-Compatible" content="IE=edge" />

  <link rel="shortcut icon" sizes="16x16 32x32 48x48" href="<%= assetPath %>vendor/govuk-frontend/images/favicon.ico" type="image/x-icon" />
  <link rel="mask-icon" href="<%= assetPath %>vendor/govuk-frontend/images/govuk-mask-icon.svg" color="#0b0c0c">
  <link rel="apple-touch-icon" sizes="180x180" href="<%= assetPath %>vendor/govuk-frontend/images/govuk-apple-touch-icon-180x180.png">
  <link rel="apple-touch-icon" sizes="167x167" href="<%= assetPath %>vendor/govuk-frontend/images/govuk-apple-touch-icon-167x167.png">
  <link rel="apple-touch-icon" sizes="152x152" href="<%= assetPath %>vendor/govuk-frontend/images/govuk-apple-touch-icon-152x152.png">
  <link rel="apple-touch-icon" href="<%= assetPath %>vendor/govuk-frontend/images/govuk-apple-touch-icon.png">

  <link href="<%= assetPath %>stylesheets/application.css" rel="stylesheet" />


  <meta property="og:image" content="<%= assetPath %>vendor/govuk-frontend/images/govuk-opengraph-image.png">

  <% if (isSubmitMetaRedirectUrl) { %>
  <meta http-equiv="refresh" content="<%= waitTimeBeforeMetaRedirectInSeconds %>;url=<%= metaRedirectUrl %>" />
  <% } %>
</head>
