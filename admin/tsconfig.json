{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noImplicitAny": false,
    "outDir": "./dist",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/*",
        "types/*"
      ]
    }
  },
  "include": [
    "."
  ],
  "exclude": [
    "availability",
    "config",
    "controllers",
    "error-types",
    "helpers",
    "lib",
    "models",
    "routes",
    "services",
    "app.js",
    "config.js",
    "server.js",
    "config.js",
    ".nyc_output",
    ".vscode",
    "assets",
    "bin",
    "coverage",
    "data",
    "dist",
    "public",
    "spec",
    "tests-integration",
    "views",
    "karma.conf.js",
    "newrelic.js",
    "run-jasmine.js",
    "wallaby.conf.js"
  ]
}
