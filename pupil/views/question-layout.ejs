<%- partial('partials/_top_of_page') %>
<!DOCTYPE html>
<!--[if lt IE 9]><html class="lte-ie8" lang="<% if (htmlLang) { %><%= htmlLang %><% } else { %>en<% } %>"><![endif]-->
<!--[if gt IE 8]><!--><html lang="<% if (htmlLang) { %><%= htmlLang %><% } else { %>en<% } %>"><!--<![endif]-->
<head>
    <meta charset="utf-8" />
    <title><%- partial('partials/_page_title') %></title>

    <!--[if gt IE 8]><!--><link href="<%= govukTemplateAssetPath %>/stylesheets/govuk-template-no-footer.css?0.19.2" media="screen" rel="stylesheet" /><!--<![endif]-->
    <!--[if IE 6]><link href="<%= govukTemplateAssetPath %>stylesheets/govuk-template-ie6.css?0.19.2" media="screen" rel="stylesheet" /><![endif]-->
    <!--[if IE 7]><link href="<%= govukTemplateAssetPath %>stylesheets/govuk-template-ie7.css?0.19.2" media="screen" rel="stylesheet" /><![endif]-->
    <!--[if IE 8]><link href="<%= govukTemplateAssetPath %>stylesheets/govuk-template-ie8.css?0.19.2" media="screen" rel="stylesheet" /><![endif]-->
    <link href="<%= govukTemplateAssetPath %>stylesheets/govuk-template-print.css?0.19.2" media="print" rel="stylesheet" />

    <!--[if IE 8]><link href="<%= govukTemplateAssetPath %>stylesheets/fonts-ie8.css?0.19.2" media="all" rel="stylesheet" /><![endif]-->
    <!--[if gte IE 9]><!--><link href="<%= govukTemplateAssetPath %>stylesheets/fonts.css?0.19.2" media="all" rel="stylesheet" /><!--<![endif]-->
    <!--[if lt IE 9]><script src="<%= govukTemplateAssetPath %>javascripts/ie.js?0.19.2"></script><![endif]-->

    <link rel="shortcut icon" href="<%= govukTemplateAssetPath %>images/favicon.ico?0.19.2" type="image/x-icon" />
    <link rel="mask-icon" href="<%= govukTemplateAssetPath %>images/gov.uk_logotype_crown.svg?0.19.2" color="#0b0c0c">
    <link rel="apple-touch-icon-precomposed apple-touch-icon" sizes="152x152" href="<%= govukTemplateAssetPath %>images/apple-touch-icon-152x152.png?0.19.2">
    <link rel="apple-touch-icon-precomposed apple-touch-icon" sizes="120x120" href="<%= govukTemplateAssetPath %>images/apple-touch-icon-120x120.png?0.19.2">
    <link rel="apple-touch-icon-precomposed apple-touch-icon" sizes="76x76" href="<%= govukTemplateAssetPath %>images/apple-touch-icon-76x76.png?0.19.2">
    <link rel="apple-touch-icon-precomposed apple-touch-icon" href="<%= govukTemplateAssetPath %>images/apple-touch-icon-60x60.png?0.19.2">

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:image" content="<%= govukTemplateAssetPath %>images/opengraph-image.png?0.19.2">

    <%- partial('partials/_head') %>
</head>

<body class="<%= bodyClasses %>">
<script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

<%- partial('partials/_body_start') %>

<div id="skiplink-container">
    <div>
        <a href="#content" class="skiplink"><% if (skipLinkMessage) { %><%= skipLinkMessage %><% } else { %>Skip to main content<% } %></a>
    </div>
</div>

<header role="banner" id="global-header" class="<% if (headerClass) { %><%= headerClass %><% } %>">
    <div class="header-wrapper">
        <div class="header-global">
            <div class="header-logo">
                <span title="GOV.UK" id="logo" class="content">
                    <img src="<%= govukTemplateAssetPath %>images/gov.uk_logotype_crown_invert_trans.png?0.19.2" width="36" height="32" alt=""> <% if (globalHeaderText) { %><%= globalHeaderText %><% } %>
                </span>
            </div>
            <%- partial('partials/_inside_header') %>
        </div>
        <%- partial('partials/_proposition_header_without_link') %>
    </div>
</header>

<%- partial('partials/_after_header') %>

<div id="global-header-bar"></div>

<%- partial('partials/_question_content', {body: body}) %>
<%- partial('partials/_no_js', { body: body }) %>

<div id="global-app-error" class="app-error hidden"></div>

<script src="<%= govukTemplateAssetPath %>javascripts/govuk-template.js?0.19.2"></script>

<%- partial('partials/_body_end') %>

<script>if (typeof window.GOVUK === 'undefined') document.body.className = document.body.className.replace('js-enabled', '');</script>
</body>
</html>
