{
  "name": "functions-app",
  "version": "0.0.1",
  "description": "MTC Azure Functions App Service Plan - for very long running functions",
  "main": "index.js",
  "license": "GPL-3.0",
  "scripts": {
    "start": "func host start --port 7072",
    "prestart": "func extensions install",
    "test": "yarn lint && yarn run nyc --exclude '*.spec.js' -r html jasmine **/*.spec.js */**/*.spec.js */**/**/*.spec.js",
    "lint": "standard",
    "pack": "funcpack pack .",
    "install-bindings": "func extensions install",
    "start:dev": "env $(cat disable-functions.env | grep Azure | xargs ) yarn start"
  },
  "engines": {
    "node": ">= 14"
  },
  "resolutions": {
    "**/**/lodash": "^4.17.20"
  },
  "dependencies": {
    "archiver": "^4.0.1",
    "async": "^3.2.2",
    "azure-storage": "^2.10.3",
    "bluebird": "^3.7.2",
    "csv-string": "^3.1.8",
    "device": "^0.3.9",
    "dotenv": "^8.2.0",
    "fast-csv": "^4.1.1",
    "fs-extra": "^9.0.1",
    "ioredis": "^4.16.0",
    "lz-string": "^1.4.4",
    "moment": "^2.29.2",
    "moment-duration-format": "^2.3.2",
    "mssql": "^6.1.0",
    "object-hash": "^2.0.3",
    "ramda": "^0.27.0",
    "ramda-adjunct": "^2.25.0",
    "to-bool": "^0.0.1",
    "useragent": "^2.3.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "jasmine": "^3.5.0",
    "nyc": "^15.0.0",
    "standard": "^14.3.1"
  }
}
