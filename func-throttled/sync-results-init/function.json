{
  "bindings": [
    {
      "name": "syncResultsInit",
      "type": "timerTrigger",
      "direction": "in",
      "timerDescription": "{second} {minute} {hour} {day} {month} {day-of-week}",
      "x-prod-schedule":  "0 30 17 * * *",
      "x-prod-description": "Every Day at 6pm GMT",
      "schedule": "0 0 18 * * *"
    },
    {
      "direction": "out",
      "type": "serviceBus",
      "name": "checkCompletion",
      "queueName": "check-completion",
      "connection": "AZURE_SERVICE_BUS_CONNECTION_STRING"
    }
  ],
  "scriptFile": "../dist/functions-throttled/sync-results-init/index.js"
}
