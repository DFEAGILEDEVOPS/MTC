{
  "name": "tslib",
  "version": "0.1.0",
  "private": false,
  "license": "GPL-3.0",
  "scripts": {
    "build": "tsc -b --v",
    "build:watch": "tsc -b --v -w",
    "rebuild": "yarn clean && yarn build",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "yarn lint && yarn jest",
    "test:throttle": "yarn lint && yarn jest --maxWorkers=2",
    "test:watch": "yarn watch",
    "watch:all": "yarn jest --watchAll",
    "watch": "yarn jest --watch",
    "clean": "rm -rf ./dist",
    "coverage": "yarn jest --coverage",
    "tests:integration": "yarn test:integration",
    "test:integration": "yarn jest --config ./src/tests-integration/jest.integration.config.js",
    "watch:integration": "yarn jest --watch --config ./src/tests-integration/jest.integration.config.js"
  },
  "engines": {
    "node": ">= 14"
  },
  "description": "Root MTC Typescript project.  Contains all shared components and azure function implementations",
  "resolutions": {
    "**/**/lodash": "^4.17.21"
  },
  "devDependencies": {
    "@azure/functions": "^1.2.3",
    "@types/adm-zip": "^0.4.34",
    "@types/async": "^3.2.7",
    "@types/bluebird": "^3.5.36",
    "@types/faker": "^5.5.8",
    "@types/he": "^1.1.2",
    "@types/ioredis": "^4.27.4",
    "@types/jest": "^26.0.15",
    "@types/lz-string": "^1.3.33",
    "@types/mssql": "7.1.3",
    "@types/node": "14",
    "@types/ramda": "^0.28.15",
    "@types/ua-parser-js": "^0.7.36",
    "@types/unzipper": "^0.10.4",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "bcryptjs": "^2.4.3",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "5.1.0",
    "he": "^1.2.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@azure/core-asynciterator-polyfill": "^1.0.2",
    "@azure/data-tables": "^12.1.2",
    "@azure/service-bus": "^7.7.3",
    "@azure/storage-blob": "^12.12.0",
    "@azure/storage-queue": "^12.11.0",
    "adm-zip": "^0.5.10",
    "applicationinsights": "^2.3.5",
    "async": "^3.2.4",
    "axios": "^0.27.2",
    "bluebird": "^3.7.0",
    "csv-string": "^4.1.0",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "fast-xml-parser": "^3.20.0",
    "ioredis": "^4.27.9",
    "lz-string": "^1.4.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "mssql": "^8.1.4",
    "ramda": "^0.28.0",
    "ramda-adjunct": "^3.4.0",
    "random-number-csprng": "^1.0.2",
    "ua-parser-js": "^1.0.33",
    "uuid": "^8.3.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/tests-integration/"
    ]
  },
  "globals": {
    "ts-jest": {
      "isolatedModules": true
    }
  }
}
