{
  "name": "tslib",
  "version": "0.1.0",
  "private": false,
  "license": "GPL-3.0",
  "scripts": {
    "build": "tsc -b --v",
    "build:watch": "tsc -b --v -w",
    "rebuild": "yarn clean && yarn build",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "yarn lint && yarn jest",
    "test:throttle": "yarn lint && yarn jest --maxWorkers=2",
    "test:watch": "yarn watch",
    "watch:all": "yarn jest --watchAll",
    "watch": "yarn jest --watch",
    "clean": "rm -rf ./dist",
    "coverage": "yarn jest --coverage",
    "tests:integration": "yarn jest --config ./src/tests-integration/jest.integration.config.js",
    "watch:integration": "yarn jest --watch --config ./src/tests-integration/jest.integration.config.js"
  },
  "engines": {
    "node": ">= 10.14.1"
  },
  "description": "Root MTC Typescript project.  Contains all shared components and azure function implementations",
  "resolutions": {
    "**/**/lodash": "^4.17.20"
  },
  "devDependencies": {
    "@azure/functions": "^1.0.3",
    "@types/adm-zip": "^0.4.33",
    "@types/bluebird": "^3.5.33",
    "@types/he": "^1.1.1",
    "@types/ioredis": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/lz-string": "^1.3.33",
    "@types/mssql": "6.0.7",
    "@types/node": "^14.14.10",
    "@types/ramda": "^0.27.32",
    "@types/ua-parser-js": "^0.7.33",
    "@types/unzipper": "^0.10.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "^4.8.2",
    "bcryptjs": "^2.4.3",
    "eslint": "^7.14.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "5.0.0",
    "he": "^1.2.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@azure/service-bus": "^1.1.2",
    "adm-zip": "^0.5.2",
    "applicationinsights": "^1.8.8",
    "axios": "^0.21.1",
    "azure-storage": "^2.10.3",
    "bluebird": "^3.7.0",
    "csv-string": "^4.0.1",
    "dotenv": "^8.1.0",
    "fast-xml-parser": "^3.17.4",
    "ioredis": "^4.19.2",
    "lz-string": "^1.4.4",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.32",
    "mssql": "^6.2.1",
    "ramda": "^0.27.0",
    "ramda-adjunct": "^2.21.0",
    "random-number-csprng": "^1.0.2",
    "ua-parser-js": "^0.7.24",
    "uuid": "^8.3.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/tests-integration/"
    ]
  },
  "globals": {
    "ts-jest": {
      "isolatedModules": true
    }
  }
}
