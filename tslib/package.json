{
  "name": "tslib",
  "version": "1.0.0",
  "private": false,
  "license": "GPL-3.0",
  "scripts": {
    "build:watch": "tsc -b --v -w",
    "build": "tsc -b --v",
    "clean": "rm -rf ./dist",
    "cleanbuild": "yarn clean && yarn build",
    "coverage": "yarn test:coverage",
    "lint:fix": "yarn eslint . --fix",
    "lint": "yarn eslint . ",
    "rebuild": "yarn cleanbuild",
    "test:coverage": "yarn jest --coverage",
    "test:integration": "yarn jest --config ./src/tests-integration/jest.integration.config.js",
    "test:throttle": "yarn lint && yarn jest --maxWorkers=2",
    "test:watch": "yarn watch",
    "test": "yarn lint && yarn jest",
    "tests:integration": "yarn test:integration",
    "watch:all": "yarn jest --watchAll",
    "watch:integration": "yarn jest --watch --config ./src/tests-integration/jest.integration.config.js",
    "watch": "yarn jest --watch"
  },
  "engines": {
    "node": ">= 18"
  },
  "description": "Root MTC Typescript project.  Contains all shared components and azure function implementations",
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@faker-js/faker": "9.3.0",
    "@types/adm-zip": "^0.5.0",
    "@types/async": "^3.2.7",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.10.0",
    "@types/ramda": "^0.30.2",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "bcryptjs": "^2.4.3",
    "eslint": "9.14.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-n": "^17.14.0",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^15.13.0",
    "he": "^1.2.0",
    "jest": "^29.6.1",
    "node-mocks-http": "^1.12.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  },
  "dependencies": {
    "@azure/data-tables": "^13.2.1",
    "@azure/functions": "^4.6.0",
    "@azure/service-bus": "^7.9.4",
    "@azure/storage-blob": "~12.26.0",
    "applicationinsights": "^2.9.6",
    "async": "^3.2.5",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "csv-string": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "fflate": "^0.8.2",
    "helmet": "8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lz-string": "^1.4.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.46",
    "morgan": "^1.10.0",
    "mssql": "^11.0.1",
    "ramda": "^0.30.1",
    "ramda-adjunct": "^5.1.0",
    "ua-parser-js": "^2.0.0",
    "uuid": "^11.0.3",
    "winston": "^3.16.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/tests-integration/"
