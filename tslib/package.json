{
  "name": "tslib",
  "version": "1.0.0",
  "private": false,
  "license": "GPL-3.0",
  "scripts": {
    "build": "tsc -b --v",
    "build:watch": "tsc -b --v -w",
    "rebuild": "yarn clean && yarn build",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "yarn lint && yarn jest",
    "test:throttle": "yarn lint && yarn jest --maxWorkers=2",
    "test:watch": "yarn watch",
    "watch:all": "yarn jest --watchAll",
    "watch": "yarn jest --watch",
    "clean": "rm -rf ./dist",
    "coverage": "yarn jest --coverage",
    "tests:integration": "yarn test:integration",
    "test:integration": "yarn jest --config ./src/tests-integration/jest.integration.config.js",
    "watch:integration": "yarn jest --watch --config ./src/tests-integration/jest.integration.config.js"
  },
  "engines": {
    "node": ">= 18"
  },
  "description": "Root MTC Typescript project.  Contains all shared components and azure function implementations",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/adm-zip": "^0.5.0",
    "@types/async": "^3.2.7",
    "@types/bluebird": "^3.5.38",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/he": "^1.1.2",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/mssql": "9.1.5",
    "@types/node": "18.11.9",
    "@types/ramda": "^0.28.15",
    "@types/ua-parser-js": "^0.7.36",
    "@types/unzipper": "^0.10.4",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "bcryptjs": "^2.4.3",
    "eslint": "^8.44.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "6.1.1",
    "he": "^1.2.0",
    "jest": "29.6.1",
    "node-mocks-http": "^1.12.2",
    "ts-jest": "29.1.2",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@azure/core-asynciterator-polyfill": "^1.0.2",
    "@azure/data-tables": "^13.2.1",
    "@azure/functions": "^4.5.0",
    "@azure/service-bus": "^7.9.4",
    "@azure/storage-blob": "^12.23.0",
    "@azure/storage-queue": "^12.22.0",
    "adm-zip": "^0.5.12",
    "applicationinsights": "^2.9.5",
    "async": "^3.2.5",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "csv-string": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fast-xml-parser": "^4.4.1",
    "helmet": "6.0.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lz-string": "^1.4.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "morgan": "^1.10.0",
    "mssql": "^10.0.2",
    "ramda": "^0.28.0",
    "ramda-adjunct": "^3.4.0",
    "random-number-csprng": "^1.0.2",
    "ua-parser-js": "^1.0.36",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-azure-application-insights": "^4.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/tests-integration/"
    ]
  },
  "globals": {
    "ts-jest": {
      "isolatedModules": true
    }
  }
}
