{
  "name": "tslib",
  "version": "0.1.0",
  "private": false,
  "license": "GPL-3.0",
  "scripts": {
    "build": "tsc -b --v",
    "build:watch": "tsc -b --v -w",
    "rebuild": "yarn clean && yarn build",
    "lint": "standard && tslint --project .",
    "test": "yarn lint && yarn jest",
    "test:throttle": "yarn lint && yarn jest --maxWorkers=2",
    "test:watch": "yarn watch",
    "watch:all": "yarn jest --watchAll",
    "watch": "yarn jest --watch",
    "clean": "rm -rf ./dist",
    "coverage": "yarn jest --coverage",
    "tests:integration": "yarn jest --config ./src/tests-integration/jest.integration.config.js",
    "watch:integration": "yarn jest --watch --config ./src/tests-integration/jest.integration.config.js"
  },
  "engines": {
    "node": ">= 10.14.1"
  },
  "description": "Root MTC Typescript project.  Contains all shared components and azure function implementations",
  "resolutions": {
    "**/**/lodash": "^4.17.20"
  },
  "devDependencies": {
    "@azure/functions": "^1.0.3",
    "@types/adm-zip": "^0.4.33",
    "@types/bluebird": "^3.5.27",
    "@types/he": "^1.1.1",
    "@types/ioredis": "^4.0.18",
    "@types/jest": "^24.0.20",
    "@types/lz-string": "^1.3.33",
    "@types/mssql": "6.0.5",
    "@types/node": "^12.7.9",
    "@types/ramda": "^0.26.33",
    "@types/unzipper": "^0.10.3",
    "@types/uuid": "^3.4.5",
    "he": "^1.2.0",
    "jest": "^24.9.0",
    "standard": "^14.3.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.3",
    "typescript-tslint-plugin": "^0.5.4"
  },
  "dependencies": {
    "@azure/service-bus": "^1.1.2",
    "adm-zip": "^0.4.16",
    "axios": "^0.20.0",
    "azure-storage": "^2.10.3",
    "bluebird": "^3.7.0",
    "csv-string": "^4.0.1",
    "dotenv": "^8.1.0",
    "fast-xml-parser": "^3.17.4",
    "ioredis": "^4.14.1",
    "lz-string": "^1.4.4",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "mssql": "^6.2.1",
    "ramda": "^0.27.0",
    "ramda-adjunct": "^2.21.0",
    "random-number-csprng": "^1.0.2",
    "uuid": "^3.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/tests-integration/"
    ]
  },
  "globals": {
    "ts-jest": {
      "isolatedModules": true
    }
  }
}
