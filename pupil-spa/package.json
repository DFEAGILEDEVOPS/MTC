{
  "name": "pupil-spa",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "scripts": {
    "ng": "ng",
    "start": "export NODE_OPTIONS=--dns-result-order=ipv4first && yarn genconfig && yarn run gulp setSecretEnvVars && yarn build:prod && yarn run http-server ./dist -p 4200 --proxy http://localhost:4200?",
    "start:dev": "export NODE_OPTIONS=--dns-result-order=ipv4first && yarn genconfig && yarn run gulp setSecretEnvVars && ng serve --host 0.0.0.0",
    "build": "yarn clean && yarn genconfig && ng build",
    "build:prod": "yarn clean && ng build --configuration production --build-optimizer --aot",
    "clean": "rm -rf ./dist",
    "test": "yarn genconfig && ng test",
    "coverage": "yarn genconfig && ng test --no-watch --browsers ChromeHeadless --code-coverage",
    "test-single": "yarn genconfig && ng test --watch false --browsers ChromeHeadless",
    "test:single": "yarn test-single",
    "genconfig": "./gen_config.sh && mv config.json src/assets/config.json",
    "lint": "ng lint",
    "runUnsupportedBrowserTests": "./unsupported-browser-tests/run-unsupported-browser-tests.sh"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/localize": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/platform-server": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@babel/runtime": "^7.26.0",
    "@microsoft/applicationinsights-web": "^3.3.3",
    "dotenv": "^16.0.3",
    "fflate": "^0.8.2",
    "lz-string": "^1.5.0",
    "ngx-cookie-service": "^16.0.0",
    "polly-js": "^1.8.2",
    "rxjs": "^7.8.1",
    "sass": "^1.83.0",
    "tslib": "^2.5.2",
    "uuid": "^11.0.3",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular-eslint/builder": "^16",
    "@angular-eslint/eslint-plugin": "^16",
    "@angular-eslint/eslint-plugin-template": "^16",
    "@angular-eslint/schematics": "^16",
    "@angular-eslint/template-parser": "^16",
    "@angular/cli": "^16.2.16",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/language-service": "^16.2.12",
    "@types/jasmine": "^5.1.5",
    "@types/node": "20",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "axios": "^1.6.0",
    "eslint": "^8.23.0",
    "fs-extra": "^10.0.0",
    "govuk-elements-sass": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-string-replace": "^1.1.2",
    "http-server": "^14.0.0",
    "jasmine": "^5.5.0",
    "jasmine-core": "~5.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "node-sass-utils": "^1.1.2",
    "replace-in-file": "^6.3.2",
    "typescript": "~5.1.0"
