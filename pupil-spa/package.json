{
  "name": "pupil-spa",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "scripts": {
    "ng": "ng",
    "sass": "node-sass -r src/assets/shared-styles/ -o src/public/styles/",
    "sasswatch": "yarn sass && node-sass src/assets/shared-styles -wro src/public/styles/",
    "start": "yarn genconfig && yarn run gulp setSecretEnvVars && concurrently \"ng serve --host 0.0.0.0\" \"yarn sasswatch\" ",
    "startwithgovuk": "node tools/copyGovukFrontendToolkit && node tools/copyGovukElements && ng serve",
    "build": "yarn genconfig && ng build",
    "test": "yarn genconfig && ng test",
    "test-single": "yarn genconfig && ng test --watch false --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copygovuktoolkit": "node tools/copyGovukFrontendToolkit",
    "copygovukelements": "node tools/copyGovukElements",
    "postinstall": "node tools/copyGovukFrontendToolkit && node tools/copyGovukElements",
    "genconfig": "bash gen_config.sh && mv config.json src/public/config.json"
  },
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/platform-server": "^6.1.10",
    "@angular/router": "^6.1.10",
    "angulartics2": "7.5.2",
    "applicationinsights-js": "^1.0.18",
    "bluebird": "^3.5.2",
    "core-js": "^2.5.3",
    "dotenv": "^8.2.0",
    "lz-string": "https://github.com/pieroxy/lz-string#1.4.4",
    "ngx-logger": "4",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.5.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "^6.2.9",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@types/applicationinsights-js": "^1.0.5",
    "@types/jasmine": "^3.4.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^12.12.6",
    "codelyzer": "^4.1.0",
    "concurrently": "^3.5.1",
    "fs-extra": "^4.0.3",
    "govuk-elements-sass": "^3.1.1",
    "gulp": "^3.9.1",
    "gulp-string-replace": "^0.4.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.13.0",
    "node-sass-utils": "^1.1.2",
    "protractor": "^5.3.0",
    "replace-in-file": "^3.1.0",
    "sass-loader": "^6.0.7",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "2.9.2"
  }
}
