{
  "name": "pupil-spa",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "scripts": {
    "ng": "ng",
    "start": "yarn genconfig && yarn run gulp setSecretEnvVars && yarn build:prod && yarn run http-server ./dist -p 4200 --proxy http://localhost:4200?",
    "start:dev": "yarn genconfig && yarn run gulp setSecretEnvVars && ng serve --host 0.0.0.0",
    "build": "yarn clean && yarn genconfig && ng build",
    "build:prod": "yarn clean && ng build --configuration production --build-optimizer --aot",
    "clean": "rm -rf ./dist",
    "test": "yarn genconfig && ng test",
    "coverage": "yarn genconfig && ng test --no-watch --browsers ChromeHeadless --code-coverage",
    "test-single": "yarn genconfig && ng test --watch false --browsers ChromeHeadless",
    "genconfig": "./gen_config.sh && mv config.json src/assets/config.json"
  },
  "engines": {
    "node": ">= 14"
  },
  "resolutions": {
    "**/**/lodash": ">=4.17.21",
    "**/**/path-parse": ">=1.0.7"
  },
  "dependencies": {
    "@angular/animations": "^14.2.1",
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/forms": "^14.2.1",
    "@angular/localize": "14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/platform-server": "^14.2.1",
    "@angular/router": "^14.2.1",
    "@microsoft/applicationinsights-web": "^2.8.7",
    "core-js": "2.6.10",
    "dotenv": "^16.0.2",
    "lz-string": "https://github.com/pieroxy/lz-string#1.4.4",
    "ngx-cookie-service": "^14.0.1",
    "ngx-logger": "4.3.3",
    "polly-js": "^1.8.2",
    "rxjs": "^6.5.3",
    "sass": "^1.54.9",
    "tslib": "^2.0.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular/cli": "14.2.2",
    "@angular/compiler-cli": "^14.2.1",
    "@angular/language-service": "^14.2.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/lz-string": "^1.3.34",
    "@types/node": "14",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "^7.6.0",
    "fs-extra": "^10.0.0",
    "govuk-elements-sass": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-string-replace": "^1.1.2",
    "http-server": "^14.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "node-sass-utils": "^1.1.2",
    "replace-in-file": "^6.3.2",
    "ts-node": "~3.3.0",
    "typescript": "4.6.4"
  }
}
