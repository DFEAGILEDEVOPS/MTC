{
  "name": "pupil-spa",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "scripts": {
    "ng": "ng",
    "sass": "node-sass -r src/assets/shared-styles/ -o src/public/styles/",
    "sasswatch": "yarn sass && node-sass src/assets/shared-styles -wro src/public/styles/",
    "start": "yarn genconfig && yarn run gulp setSecretEnvVars && yarn build --prod && yarn run http-server dist/ -p 4200 --proxy http://localhost:4200?",
    "start:dev": "yarn genconfig && yarn run gulp setSecretEnvVars && concurrently \"ng serve --host 0.0.0.0\" \"yarn sasswatch\" ",
    "startwithgovuk": "node tools/copyGovukFrontendToolkit && node tools/copyGovukElements && ng serve",
    "build": "yarn clean && yarn genconfig && yarn sass && ng build",
    "clean": "rm -rf ./dist",
    "test": "yarn genconfig && ng test",
    "coverage": "yarn genconfig && ng test --no-watch --browsers ChromeHeadless --code-coverage",
    "test-single": "yarn sass && yarn genconfig && ng test --watch false --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copygovuktoolkit": "node tools/copyGovukFrontendToolkit",
    "copygovukelements": "node tools/copyGovukElements",
    "postinstall": "node tools/copyGovukFrontendToolkit && node tools/copyGovukElements",
    "genconfig": "bash gen_config.sh && mv config.json src/public/config.json"
  },
  "resolutions": {
    "**/**/lodash": "^4.17.21"
  },
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/localize": "10.2.5",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/platform-server": "^10.2.5",
    "@angular/router": "^10.2.5",
    "@microsoft/applicationinsights-web": "^2.5.10",
    "bluebird": "^3.7.1",
    "core-js": "2.6.10",
    "dotenv": "^10.0.0",
    "lz-string": "https://github.com/pieroxy/lz-string#1.4.4",
    "ngx-cookie-service": "^10.1.1",
    "ngx-logger": "4.3.3",
    "polly-js": "^1.8.2",
    "rxjs": "^6.5.3",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "10.2.3",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "^10.2.5",
    "@types/jasmine": "^3.4.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^6.2.0",
    "fs-extra": "^8.1.0",
    "govuk-elements-sass": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-string-replace": "^1.1.2",
    "http-server": "^0.12.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-sass": "^6.0.1",
    "node-sass-utils": "^1.1.2",
    "protractor": "~7.0.0",
    "replace-in-file": "^4.2.0",
    "sass-loader": "^6.0.7",
    "ts-node": "~3.3.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.8"
  }
}