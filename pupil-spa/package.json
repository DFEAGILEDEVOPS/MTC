{
  "name": "pupil-spa",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "scripts": {
    "ng": "ng",
    "sass": "node-sass -r src/assets/shared-styles/ -o src/public/styles/",
    "sasswatch": "yarn sass && node-sass src/assets/shared-styles -wro src/public/styles/",
    "start": "yarn genconfig && yarn run gulp setSecretEnvVars && concurrently \"ng serve --host 0.0.0.0\" \"yarn sasswatch\" ",
    "startwithgovuk": "node tools/copyGovukFrontendToolkit && node tools/copyGovukElements && ng serve",
    "build": "yarn genconfig && ng build",
    "test": "yarn genconfig && ng test",
    "test-single": "yarn genconfig && ng test --watch false --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copygovuktoolkit": "node tools/copyGovukFrontendToolkit",
    "copygovukelements": "node tools/copyGovukElements",
    "postinstall": "node tools/copyGovukFrontendToolkit && node tools/copyGovukElements",
    "genconfig": "bash gen_config.sh && mv config.json src/public/config.json"
  },
  "dependencies": {
    "@angular/animations": "^8.2.13",
    "@angular/common": "^8.2.13",
    "@angular/compiler": "^8.2.13",
    "@angular/core": "^8.2.13",
    "@angular/forms": "^8.2.13",
    "@angular/platform-browser": "^8.2.13",
    "@angular/platform-browser-dynamic": "^8.2.13",
    "@angular/platform-server": "^8.2.13",
    "@angular/router": "^8.2.13",
    "angulartics2": "8.1.0",
    "applicationinsights-js": "^1.0.18",
    "bluebird": "^3.7.1",
    "core-js": "2.6.10",
    "dotenv": "^8.2.0",
    "lz-string": "https://github.com/pieroxy/lz-string#1.4.4",
    "ngx-logger": "4",
    "rxjs": "^6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.8",
    "@angular/cli": "8.3.8",
    "@angular/compiler-cli": "^8.2.13",
    "@angular/language-service": "^8.2.13",
    "@types/applicationinsights-js": "^1.0.5",
    "@types/jasmine": "^3.4.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^12.12.6",
    "codelyzer": "^5.0.1",
    "concurrently": "^5.0.0",
    "fs-extra": "^8.1.0",
    "govuk-elements-sass": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-string-replace": "^1.1.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.13.0",
    "node-sass-utils": "^1.1.2",
    "protractor": "^5.3.0",
    "replace-in-file": "^4.2.0",
    "sass-loader": "^6.0.7",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "3.5.3"
  }
}
