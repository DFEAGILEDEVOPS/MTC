{
  "name": "pupil-spa",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "scripts": {
    "ng": "ng",
    "start": "yarn genconfig && yarn run gulp setSecretEnvVars && yarn build:prod && yarn run http-server ./dist -p 4200 --proxy http://localhost:4200?",
    "start:dev": "yarn genconfig && yarn run gulp setSecretEnvVars && ng serve --host 0.0.0.0",
    "build": "yarn clean && yarn genconfig && ng build",
    "build:prod": "yarn clean && ng build --configuration production --build-optimizer --aot",
    "clean": "rm -rf ./dist",
    "test": "yarn genconfig && ng test",
    "coverage": "yarn genconfig && ng test --no-watch --browsers ChromeHeadless --code-coverage",
    "test-single": "yarn genconfig && ng test --watch false --browsers ChromeHeadless",
    "genconfig": "./gen_config.sh && mv config.json src/assets/config.json",
    "lint": "ng lint",
    "runUnsupportedBrowserTests": "./unsupported-browser-tests/run-unsupported-browser-tests.sh"
  },
  "engines": {
    "node": ">= 16"
  },
  "resolutions": {
    "**/**/lodash": ">=4.17.21",
    "**/**/path-parse": ">=1.0.7"
  },
  "dependencies": {
    "@angular/animations": "^14.2.10",
    "@angular/common": "^14.2.10",
    "@angular/compiler": "^14.2.10",
    "@angular/core": "^14.2.10",
    "@angular/forms": "^14.2.10",
    "@angular/localize": "^14.2.12",
    "@angular/platform-browser": "^14.2.10",
    "@angular/platform-browser-dynamic": "^14.2.10",
    "@angular/platform-server": "^14.2.10",
    "@angular/router": "^14.2.10",
    "@babel/runtime": "^7.21.0",
    "@microsoft/applicationinsights-web": "^2.8.15",
    "core-js": "2.6.12",
    "dotenv": "^16.0.3",
    "lz-string": "^1.5.0",
    "ngx-cookie-service": "^14.0.1",
    "polly-js": "^1.8.2",
    "rxjs": "^6.5.3",
    "sass": "^1.66.1",
    "tslib": "^2.5.2",
    "uuid": "^9.0.0",
    "zone.js": "~0.13.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.5",
    "@angular-eslint/builder": "^14.1.2",
    "@angular-eslint/eslint-plugin": "^14.1.2",
    "@angular-eslint/eslint-plugin-template": "^14.1.2",
    "@angular-eslint/schematics": "^14.1.2",
    "@angular-eslint/template-parser": "^14.1.2",
    "@angular/cli": "^14.2.5",
    "@angular/compiler-cli": "^14.2.5",
    "@angular/language-service": "^14.2.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "16",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "axios": "^1.1.3",
    "eslint": "^8.23.0",
    "fs-extra": "^10.0.0",
    "govuk-elements-sass": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-string-replace": "^1.1.2",
    "http-server": "^14.0.0",
    "jasmine": "^4.4.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
