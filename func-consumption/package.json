{
  "name": "func-consumption",
  "description": "runtime host for functions defined in tslib that are designed to run on a consumption plan in Azure",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "engines": {
    "node": ">= 10.14.1"
  },
  "scripts": {
    "build": "yarn sync && yarn run gulp && func extensions install",
    "prod:clean": "rm -rf ./node_modules && yarn install --frozen-lockfile --production",
    "rebuild": "yarn clean && yarn build",
    "sync": "node ./sync-tslib-deps.js && yarn install",
    "delete-specs": "find . -name \"*.spec.js\" -type f -delete",
    "clean": "rm -rf ./dist",
    "prestart": "yarn build",
    "start:host": "func start",
    "start": "yarn start:host",
    "start:dev": "concurrently -r 'env $(cat disable-functions.env | grep Azure | xargs ) yarn start' 'gulp watch'"
  },
  "resolutions": {
    "**/**/lodash": "^4.17.20"
  },
  "devDependencies": {
    "@azure/functions": "^1.0.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.27",
    "@types/ioredis": "^4.0.18",
    "@types/lz-string": "^1.3.33",
    "@types/mssql": "^6.0.7",
    "@types/node": "^10.17.27",
    "@types/ramda": "^0.27.11",
    "@types/uuid": "^8.3.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "edit-json-file": "^1.3.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^5.0.1",
    "gulp-yarn": "^2.0.0",
    "standard": "^16.0.3",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.1.3",
    "typescript-tslint-plugin": "^1.0.1"
  },
  "dependencies": {
    "@azure/service-bus": "^1.1.2",
    "adm-zip": "^0.5.0",
    "applicationinsights": "^1.8.8",
    "axios": "^0.21.1",
    "azure-storage": "^2.10.3",
    "bluebird": "^3.7.0",
    "csv-string": "^4.0.1",
    "dotenv": "^8.1.0",
    "fast-xml-parser": "^3.17.4",
    "ioredis": "^4.19.2",
    "lz-string": "^1.4.4",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.32",
    "mssql": "^6.2.1",
    "ramda": "^0.27.0",
    "ramda-adjunct": "^2.21.0",
    "random-number-csprng": "^1.0.2",
    "ua-parser-js": "^0.7.23",
    "uuid": "^8.3.1"
  }
}