{
    "version":  "2.0",
    "extensions": {
      "http": {
          "routePrefix": "api",
          "maxOutstandingRequests": -1,
          "maxConcurrentRequests": -1,
          "dynamicThrottlesEnabled": false,
          "hsts": {
              "isEnabled": true,
              "maxAge": "31536000",
              "includeSubDomains": true,
              "preload": true
          }
      }
  }
}
